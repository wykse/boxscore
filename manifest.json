{
  "manifest_version": 2,
  "name": "boxscore",
  "description": "Box score for NBA games on YouTube.",
  "version": "0.0.2",
  "icons": {
    "64": "icons/icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/*"
      ],
      "js": [
        "content_script.js"
      ]
    }
  ],
  "permissions": [
    "*://*.nba.com/*"
  ]
}